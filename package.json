{
  "name": "gerenciador_aws",
  "private": true,
  "version": "1.0.2",
  "main": "electron.js",
  "type": "module",
  "homepage": "https://github.com/alexandre-oliveira-dev/gerenciador_credenciais_aws",
  "author": {
    "email": "alexandredev.frontend@gmail.com",
    "name": "Alexandre"
  },
  "build": {
    "appId": "aws-manager",
    "asar": false,
    "productName": "Gerenciador de credenciais Aws",
    "files": [
      "dist/**/*",
      "api/**/*",
      "electron.js",
      "public/*",
      "!api/credentials.json"
    ],
    "directories": {
      "output": "exec-files/"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "linux": {
      "target": [
        "deb"
      ],
      "icon": "public/icon.png"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "startapi": "node api/index.js",
    "start": "NODE_ENV=production electron .",
    "electron": "NODE_ENV=development electron .",
    "start:dev:all": "concurrently \"node api/index.js\" \"npm run build\" \"npm run dev\" \"npm run electron\"",
    "start:all": "concurrently  \"npm run start\"",
    "build-app": "electron-builder --linux --win",
    "deploy-win": "npm run build && electron-builder --win",
    "install-ubuntu": "sudo dpkg -i ./exec-files/gerenciador_aws_1.0.2_amd64.deb",
    "delete-not-necessary-files": "rm -rf exec-files/*.yml exec-files/linux-unpacked exec-files/win-unpacked exec-files/*.blockmap exec-files/*.yaml",
    "deploy": "npm run build && npm run build-app && yarn install-ubuntu && yarn delete-not-necessary-files"
  },
  "dependencies": {
    "antd": "^5.24.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-query": "^3.39.3",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "electron": "^36.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
